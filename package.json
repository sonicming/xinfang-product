{
  "name": "xinfang-product",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "md5": "^2.2.1",
    "babel-polyfill": "^6.26.0",
    "element-china-area-data": "^3.1.0",
    "element-theme-chalk": "^2.4.1",
    "element-ui": "^2.4.0",
    "font-awesome": "^4.7.0",
    "localforage": "^1.5.0",
    "moment": "^2.18.1",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vue-element-dialog-draggable": "^1.0.3",
    "vuex": "^3.0.1",
    "vue-moment": "^2.0.2",
    "vue-progressbar": "^0.7.5",
    "vue2-filters": "^0.3.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.11",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.11",
    "@vue/cli-service": "^3.0.0-rc.11",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "lint-staged": "^7.2.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "generator-star-spacing": "off",
      "no-empty": "off",
      "no-unused-vars": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
